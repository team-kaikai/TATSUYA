
<div class = "user-index-wrapper">
	<div class ="container">
		<div class="row">
	 		<div class="col">
				<h1>User一覧</h1>
					<table class="table rable-striped table-hover">
						<thead>
							<tr>
								<th>id</th>
								<th>username</th>
								<th>detailname</th>
								<th colspan="3"></th>
							</tr>
						</thead>
						<tbody>
						<% @end_users.each do |end_user|  %>
						<tr>
							<td>
								<p>ID<span style="font-size: 20px; padding-left: 30px;"><%= link_to end_user.id,admins_end_user_path(end_user) %></span></p>
							</td>
							<td>
								<p>名前<span style="font-size: 20px; padding-left: 30px;"><%= end_user.first_name %><%= end_user.last_name %></span></p>
							</td>
							<td>
								<p>カナ<span style="font-size: 20px; padding-left: 30px;"><%= end_user.details_first_name %><%= end_user.details_last_name %></span></p>
							</td>
						    <td><%= link_to "edit", edit_admins_end_user_path(end_user) %></td>
						</tr>
						<% end  %>
						</tbody>
					</table>
			</div>
		</div>
	</div>
</div>
