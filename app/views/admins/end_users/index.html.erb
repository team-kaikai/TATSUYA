
<div class = "user-index-wrapper">
	<div class ="container">
		<div class="row">
	 		<div class="col">
				<h1>User一覧</h1>
					<table class="table rable-striped table-hover">
						<thead>
							<tr>
								<th>id</th>
								<th>username</th>
								<th>detailname</th>
								<th colspan="3"></th>
							</tr>
						</thead>
						<tbody>
						<% @end_users.each do |end_user|  %>
							<tr>
							<td>
								<p>ID<%= link_to end_user.id,end_user_path(end_user) %></p>
							</td>
							<td>
								<p>名前<%= end_user.first_name %><%= end_user.last_name %></p>
							</td>
							<td>
								<p>カナ<%= end_user.details_first_name %><%= end_user.details_last_name %></p>
							</td>
						</tr>
							<% end  %>
						</tbody>
					</table>
			</div>
		</div>
	</div>
</div>
