
<div class = "user-index-wrapper">
	<div class ="container">
		<div class="row">
	 		<div class="col">
				<h1>User一覧</h1>
					<table class="table rable-striped table-hover">
						<thead>
							<tr>
								<th>id</th>
								<th>username</th>
								<th>detailname</th>
								<th colspan="3"></th>
							</tr>
						</thead>
						<tbody>
						<%# @users.each   %>
							<tr>
							<td>
								<p>ID<%#= link_to user.id,user_path(user) %></p>
							</td>
							<td>
								<p>名前<%#= user.first_name %><%#= user.last_name %></p>
							</td>
							<td>
								<p>カナ<%#= user.details_first_name %><%#= user.details_last_name %></p>
							</td>
						</tr>
							<%# end  %>
						</tbody>
					</table>
			</div>
		</div>
	</div>
</div>
