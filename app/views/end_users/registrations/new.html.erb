<div class="devise-wrapper">
  <div class="container">

    <h2 class="devise-title">Sign up</h2>

    <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
      <%= render "devise/shared/error_messages", resource: resource %>


      <div class="field">
        <%= f.label :性,class: "devise-font" %><br />
        <%= f.text_field :first_name,autofocus: true,class: "devise-box" %>
      </div>

      <div class="field">
        <%= f.label :名,class: "devise-font" %><br />
        <%= f.text_field :last_name,class: "devise-box" %>
      </div>

      <div class="field">
        <%= f.label :性かな,class: "devise-font" %><br />
        <%= f.text_field :details_first_name,class: "devise-box" %>
      </div>

      <div class="field">
        <%= f.label :名かな,class: "devise-font" %><br />
        <%= f.text_field :details_last_name,class: "devise-box" %>
      </div>

      <div class="field">
        <%= f.label :郵便番号,class: "devise-font" %><br />
        <%= f.text_field :postal_code,class: "devise-box" %>
      </div>

      <div class="field">
        <%= f.label :住所,class: "devise-font" %><br />
        <%= f.text_field :address,class: "devise-box" %>
      </div>

      <div class="field">
        <%= f.label :電話番号,class: "devise-font" %><br />
        <%= f.text_field :tel,class: "devise-box" %>
      </div>

      <div class="field">
        <%= f.label :email,class: "devise-font" %><br />
        <%= f.email_field :email, autocomplete: "email",class: "devise-box" %>
      </div>

      <div class="field">
        <%= f.label :password,class: "devise-font" %>
        <% if @minimum_password_length %>
        <em>(<%= @minimum_password_length %> characters minimum)</em>
        <% end %><br />
        <%= f.password_field :password, autocomplete: "new-password",class: "devise-box" %>
      </div>

      <div class="field">
        <%= f.label :パスワード再入力,class: "devise-font" %><br />
        <%= f.password_field :password_confirmation, autocomplete: "new-password",class: "devise-box" %>
      </div>

      <div class="actions">
        <%= f.submit "Sign up" %>
      </div>
    <% end %>

    <%= render "end_users/shared/links" %>
  </div>
</div>


