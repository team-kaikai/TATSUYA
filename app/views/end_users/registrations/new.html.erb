<div class="endusers-signup-wrapper">
  <div class="container" style="text-align: center;">

      <h2>Sign up</h2>
        <div class="row" style="padding-bottom: 10px;">
          <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
            <%= render "devise/shared/error_messages", resource: resource %>
          <div class="col-xs-4 col-xs-offset-1">
            <div class="field">
              <%= f.label :first_name %>
              <%= f.email_field :first_name, autofocus: true, autocomplete: "first_name" %>
            </div>
          </div>
          <div class="col-xs-4">
            <div class="field">
              <%= f.label :last_name %>
              <%= f.email_field :last_name, autofocus: true, autocomplete: "last_name" %>
            </div>
          </div>
        </div>
        <div class="row">
          <div class="col-xs-4 col-xs-offset-1">
            <div class="field">
              <%= f.label :details_first_name %>
              <%= f.email_field :details_first_name, autofocus: true, autocomplete: "details_first_name" %>
            </div>
          </div>
          <div class="col-xs-4">
            <div class="field">
              <%= f.label :details_last_name %>
              <%= f.email_field :details_last_name, autofocus: true, autocomplete: "details_last_name" %>
            </div>
          </div>
        </div>

  <div class="field">
    <%= f.label :postal_code %><br />
    <%= f.email_field :postal_code, autofocus: true, autocomplete: "postal_code" %>
  </div>

  <div class="field">
    <%= f.label :postal_code %><br />
    <%= f.email_field :postal_code, autofocus: true, autocomplete: "postal_code" %>
  </div>

  <div class="field">
    <%= f.label :address %><br />
    <%= f.email_field :address, autofocus: true, autocomplete: "address" %>
  </div>

  <div class="field">
    <%= f.label :tel %><br />
    <%= f.email_field :tel, autofocus: true, autocomplete: "tel" %>
  </div>

  <div class="field">
    <%= f.label :password %>
    <% if @minimum_password_length %>
    <em>(<%= @minimum_password_length %> characters minimum)</em>
    <% end %><br />
    <%= f.password_field :password, autocomplete: "new-password" %>
  </div>

  <div class="field">
    <%= f.label :password_confirmation %><br />
    <%= f.password_field :password_confirmation, autocomplete: "new-password" %>
  </div>

  <div class="actions">
    <%= f.submit "Sign up" %>
  </div>
<% end %>
<%= render "end_users/shared/links" %>
    </div>
  </div>
</div>


